    "require": {
        "php": "^7.3|^8.0",
        "doctrine/annotations": "^1.11",
        "unitedprint/serializer": "2.0.x-dev",
        "guzzlehttp/guzzle": "^6.3|^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "phpstan/phpstan": "^0.12.88",
        "friendsofphp/php-cs-fixer": "^2.19"
