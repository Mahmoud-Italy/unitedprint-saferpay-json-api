{
    "name": "unitedprint/saferpay-json-api",
    "type": "library",
    "description": "A php library to use the json api of Saferpay",
    "keywords": ["saferpay"],
    "homepage": "",
    "license": "MIT",
    "authors": [
        {
            "name": "Mahmoud Ahmed",
            "email": "mahmoud.ahmed@unitedprint.com"
        },
        {
            "name": "Additional contributors",
            "homepage": ""
        }
